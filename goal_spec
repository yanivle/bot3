# Preferred time = 7 but 8 ok too. These are the only 2 valid times.

BASE_INPUT(-) PRIORITY=-1
>>>NOT WRONGS<<<
PREFERRED_TIME=7pm
PARTY_SIZE=5
DATE=tomorrow
FIRST_NAME=Yaniv
LAST_NAME=Leviathan
PHONE_NUMBER=2125655611

---

RESERVATION_MADE_BASE(BASE_INPUT) PRIORITY=-1
AGREED_TIME=7pm
PARTY_SIZE=5
DATE=tomorrow
CONFIRM=True
>>>NOT WRONGS<<<
AVAILABILITY[TIME=7pm]=True

---

RESERVATION_MADE(RESERVATION_MADE_BASE) PRIORITY=100
>>>OR<<<
FIRST_NAME=Yaniv
LAST_NAME=Leviathan
PHONE_NUMBER=2125655611

---
RESERVATION_MADE_8PM(RESERVATION_MADE_BASE) PRIORITY=90
AGREED_TIME=8pm
>>>OR<<<
FIRST_NAME=Yaniv
LAST_NAME=Leviathan
PHONE_NUMBER=2125655611

---
NO_RESERVATIONS_BASE(BASE_INPUT) PRIORITY=10
>>>OR<<<
RESERVATIONS_ACCEPTED=False
RESERVATIONS_ACCEPTED[PARTY_SIZE=5]=False
RESERVATIONS_ACCEPTED[TIME=7pm]=False
>>>OR<<<
ESTIMATED_WAIT=short
ESTIMATED_WAIT=long
ESTIMATED_WAIT=unknown
