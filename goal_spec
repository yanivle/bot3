# Preferred time = 7 but 8 ok too. These are the only 2 valid times.

BASE_INPUT(-) PRIORITY=-1000
>>>NOT WRONGS<<<
TIME=7pm
PARTY_SIZE=5
DATE=tomorrow
FIRST_NAME=Yaniv
LAST_NAME=Leviathan
PHONE_NUMBER=2125655611

---

RESERVATION_MADE_BASE(BASE_INPUT) PRIORITY=-1000
TIME=7pm
PARTY_SIZE=5
DATE=tomorrow
CONFIRM=True

---

RESERVATION_MADE(RESERVATION_MADE_BASE) PRIORITY=100
>>>OR<<<
FIRST_NAME=Yaniv
LAST_NAME=Leviathan
PHONE_NUMBER=2125655611

---

RESERVATION_MADE_8PM(RESERVATION_MADE_BASE) PRIORITY=90
TIME=8pm
>>>OR<<<
FIRST_NAME=Yaniv
LAST_NAME=Leviathan
PHONE_NUMBER=2125655611

---
NO_RESERVATIONS_BASE(BASE_INPUT) PRIORITY=0
RESERVATIONS_ACCEPTED=False
>>>OR<<<
RESERVATIONS_ACCEPTED=False
RESERVATIONS_ACCEPTED[PARTY_SIZE=5]=False
RESERVATIONS_ACCEPTED[TIME=7pm]=False
>>>OR<<<
ESTIMATED_WAIT=short
ESTIMATED_WAIT=long
ESTIMATED_WAIT=unknown
