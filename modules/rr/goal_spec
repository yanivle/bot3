GROUP(H:RESERVATIONS_ACCEPTED=True, H:RESERVATIONS_ACCEPTED[PARTY_SIZE=5]=True, H:RESERVATIONS_ACCEPTED[TIME=7pm]=True, H:RESERVATIONS_ACCEPTED[MEAL=dinner]=True, H:RESERVATIONS_ACCEPTED[DATE=tomorrow]=True)

R:PARTY_SIZE=5
R:DATE=tomorrow
R:PREFERRED_TIME=7pm

GROUP(1=1, H:AVAILABILITY[TIME=7pm]=True)

GROUP(R:FIRST_NAME=Yaniv, R:LAST_NAME=Leviathan, R:PHONE_NUMBER=2125655611)

AGREED_TIME=7pm

