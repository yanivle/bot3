# Preferred time = 7 but 7:30 and 8 ok too. These are the only 3 valid times.
GOAL(RESERVATION_MADE_7PM)

PARTY_SIZE=5
DATE=tomorrow
GROUP(RESERVATIONS_ACCEPTED=True, RESERVATIONS_ACCEPTED[PARTY_SIZE=5]=True, RESERVATIONS_ACCEPTED[TIME=7pm]=True, RESERVATIONS_ACCEPTED[MEAL=dinner]=True, RESERVATIONS_ACCEPTED[DATE=tomorrow]=True)
GROUP(1=1, AVAILABILITY[TIME=7pm]=True)
GROUP(FIRST_NAME=Yaniv, LAST_NAME=Leviathan, PHONE_NUMBER=2125655611)
CONFIRM=True
AGREED_TIME=7pm

---
GOAL(RESERVATION_MADE_7:30PM)
AGREED_TIME=7:30pm
GROUP(1=1, AVAILABILITY[TIME=7:30pm]=True)
PARTY_SIZE=5
DATE=tomorrow
GROUP(RESERVATIONS_ACCEPTED=True, RESERVATIONS_ACCEPTED[PARTY_SIZE=5]=True, RESERVATIONS_ACCEPTED[TIME=7:30pm]=True, RESERVATIONS_ACCEPTED[MEAL=dinner]=True, RESERVATIONS_ACCEPTED[DATE=tomorrow]=True)
GROUP(FIRST_NAME=Yaniv, LAST_NAME=Leviathan, PHONE_NUMBER=2125655611)
CONFIRM=True

---
GOAL(RESERVATION_MADE_8PM)
AGREED_TIME=8pm
GROUP(1=1, AVAILABILITY[TIME=8pm]=True)
PARTY_SIZE=5
DATE=tomorrow
GROUP(RESERVATIONS_ACCEPTED=True, RESERVATIONS_ACCEPTED[PARTY_SIZE=5]=True, RESERVATIONS_ACCEPTED[TIME=8pm]=True, RESERVATIONS_ACCEPTED[MEAL=dinner]=True, RESERVATIONS_ACCEPTED[DATE=tomorrow]=True)
GROUP(1=1, AVAILABILITY[TIME=8pm]=True)
GROUP(FIRST_NAME=Yaniv, LAST_NAME=Leviathan, PHONE_NUMBER=2125655611)
CONFIRM=True

---
GOAL(NO_RESERVATIONS)
GROUP(RESERVATIONS_ACCEPTED=False, RESERVATIONS_ACCEPTED[PARTY_SIZE=5]=False, RESERVATIONS_ACCEPTED[MEAL=dinner]=False, RESERVATIONS_ACCEPTED[DATE=tomorrow]=False)
ESTIMATED_WAIT=*

---
GOAL(NO_AVAILABILITY_WALKIN_OK)
ESTIMATED_WAIT=*
WALKINS_ACCEPTED=True
GROUP(AVAILABILITY[TIME=7pm]=False,AVAILABILITY[TIME=7:30pm]=False,AVAILABILITY[TIME=8pm]=False)
