# Preferred time = 7 but 7:30 and 8 ok too. These are the only 3 valid times.
GOAL(RESERVATION_MADE_7PM)

R:PURPOSE=BOOK_RESERVATION

GROUP(1=1, H:AVAILABILITY=True, H:AVAILABILITY[PARTY_SIZE=5]=True, H:AVAILABILITY[TIME=7pm]=True, H:AVAILABILITY[MEAL=dinner]=True, H:AVAILABILITY[DATE=tomorrow]=True)

NOT(OR(H:CREDIT_CARD_REQUIRED=True, H:CREDIT_CARD_REQUIRED[PARTY_SIZE=5]=True))

R:PARTY_SIZE=5
R:DATE=tomorrow
R:PREFERRED_TIME=7pm

GROUP(R:FIRST_NAME=Yaniv, R:LAST_NAME=Leviathan, R:PHONE_NUMBER=2125655611)

AGREED_TIME=7pm

---
GOAL(RESERVATION_MADE_7:30PM)

R:PURPOSE=BOOK_RESERVATION

GROUP(H:AVAILABILITY=True, H:AVAILABILITY[PARTY_SIZE=5]=True, H:AVAILABILITY[TIME=7:30pm]=True, H:AVAILABILITY[MEAL=dinner]=True, H:AVAILABILITY[DATE=tomorrow]=True)

NOT(OR(H:CREDIT_CARD_REQUIRED=True, H:CREDIT_CARD_REQUIRED[PARTY_SIZE=5]=True))

R:PARTY_SIZE=5
R:DATE=tomorrow
# Note that this needs to be 7pm, as this is the preferred time, not the agreed time.
R:PREFERRED_TIME=7pm

H:AVAILABILITY[TIME=7:30pm]=True

GROUP(R:FIRST_NAME=Yaniv, R:LAST_NAME=Leviathan, R:PHONE_NUMBER=2125655611)

AGREED_TIME=7:30pm

---
GOAL(CC_REQUIRED_FOR_RESERVATION)
OR(H:CREDIT_CARD_REQUIRED=True, H:CREDIT_CARD_REQUIRED[PARTY_SIZE=5]=True)
R:WILL_CHECK_AND_CALL_BACK_LATER=True
---
GOAL(RESERVATION_MADE_8PM)

R:PURPOSE=BOOK_RESERVATION

GROUP(H:AVAILABILITY=True, H:AVAILABILITY[PARTY_SIZE=5]=True, H:AVAILABILITY[TIME=8pm]=True, H:AVAILABILITY[MEAL=dinner]=True, H:AVAILABILITY[DATE=tomorrow]=True)

NOT(OR(H:CREDIT_CARD_REQUIRED=True, H:CREDIT_CARD_REQUIRED[PARTY_SIZE=5]=True))

R:PARTY_SIZE=5
R:DATE=tomorrow
# Note that this needs to be 7pm, as this is the preferred time, not the agreed time.
R:PREFERRED_TIME=7pm

H:AVAILABILITY[TIME=8pm]=True

GROUP(R:FIRST_NAME=Yaniv, R:LAST_NAME=Leviathan, R:PHONE_NUMBER=2125655611)

AGREED_TIME=8pm

---
GOAL(NO_AVAILABILITY_WALKIN_OK)

R:PARTY_SIZE=5
R:DATE=tomorrow
# Note that this needs to be 7pm, as this is the preferred time, not the agreed time.
R:PREFERRED_TIME=7pm

OR(AND(H:AVAILABILITY[TIME=7pm]=False, H:AVAILABILITY[TIME=7:30pm]=False, H:AVAILABILITY[TIME=8pm]=False),, OR(H:AVAILABILITY=False, H:AVAILABILITY[PARTY_SIZE=5]=False, H:AVAILABILITY[MEAL=dinner]=False, H:AVAILABILITY[DATE=tomorrow]=False))
H:WALKINGS_ACCEPTED=True
H:ESTIMATED_WAIT=*
---
GOAL(NO_AVAILABILITY)

R:PARTY_SIZE=5
R:DATE=tomorrow
# Note that this needs to be 7pm, as this is the preferred time, not the agreed time.
R:PREFERRED_TIME=7pm

OR(AND(H:AVAILABILITY[TIME=7pm]=False, H:AVAILABILITY[TIME=7:30pm]=False, H:AVAILABILITY[TIME=8pm]=False),, AND(H:AVAILABILITY=False, H:AVAILABILITY[PARTY_SIZE=5]=False, H:AVAILABILITY[MEAL=dinner]=False, H:AVAILABILITY[DATE=tomorrow]=False))
